{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
    "rules": {
      "quotes": ["error", "single"],
      "indent": ["error", 2],
      "semi": ["error", "never"]
    }
}
  